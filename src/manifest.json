{
  "manifest_version": 2,
  "name": "CTNG",
  "version": "1.0",
  "description": "CTNG project.",
  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "CTNG",
    "default_popup": "index.html"
  },
  "icons": {
      "48": "icons/border-48.png"
  },
  "background": {
      "page": "index.html",
      "scripts": ["background.js", "./src/client.js"]
  },
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage",
      "unlimitedStorage"
  ]
}
