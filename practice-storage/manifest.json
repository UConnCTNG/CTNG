{
    "manifest_version": 2,
    "name": "Store Browser Info",
    "version": "1.0",

    "description": "Trying to store information",

    "background": {
        "scripts": ["background.js"]
    },

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

    "permissions": [
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage",
        "unlimitedStorage"
    ]
}